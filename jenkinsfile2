pipeline {
    agent {
        docker { image 'node:20.10.0-alpine3.19' }
    }
    stages{
        stage ('linux commands') {
        steps {
            sh 'pwd'
            //sh 'whoami'
            //sh "mkdir \$(date '+%Y%m%d_%H%M%S')"
            sh 'python3 --version'
            sh 'npm --version'
        }
        }
    }
}